# コマンド短縮
import:
    java.util.Arrays
    org.bukkit.Sound
command /m:
    trigger:
        stop
command /mm <string> <number>:
    trigger:
        stop
command /gm <string> [<player>]:
    trigger:
        stop
on command:
    cancel event
    (ops and console) contain sender
    set {_a::*} to full command split at " "
    switch command:
        case "r":
            execute player command "/skript reload scripts"
        case "a":
            execute player command "/r" # AdminMenu(player,1)
            # set gamemode of player to creative if player's gamemode is spectator
        case "mm":
            stop all sounds for player
            player.getWorld().playSound(player.getLocation(),lower case {_a::2},1,{_a::3} parsed as number)
        case "gm":
            switch {_a::2}:
                case "0" -> execute server command "/gamemode survival %{_a::3} if {_a::3} is set else player%"
                case "1" -> execute server command "/gamemode creative %{_a::3} if {_a::3} is set else player%"
                case "2" -> execute server command "/gamemode adventure %{_a::3} if {_a::3} is set else player%"
                case "3" -> execute server command "/gamemode spectator %{_a::3} if {_a::3} is set else player%"
        case "m":
            set {_a} to Arrays.asList(Sound.values())
            send "&8&o - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -" to player
            loop 7 times:
                set {_r} to {_a}.get(random integer between 0 and ({_a}.size() - 1))
                send formatted "&8&o  &8&o<cmd:/mm %{_r}% 0><&7&o0&8&o><reset> &8&o<cmd:/mm %{_r}% 1><&7&o1&8&o><reset> &8&o<cmd:/mm %{_r}% 2><&7&o2&8&o><reset> &8&o%{_r}%" to player
            send formatted "&8&o  &8&o<cmd:/m><&7&o   roll    &8&o><reset>" to player
            send "&8&o - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -" to player
        default:
            uncancel event

# ライブラリ関数
function Loc(e0:location,dy:number=0) :: string:
    return "%x of {_e0}% %{_dy} + y of {_e0}% %z of {_e0}%"
function Has(es:objects,e0:object) :: boolean:
    loop {_es::*}:
        return true if loop-value is {_e0}
    return false










